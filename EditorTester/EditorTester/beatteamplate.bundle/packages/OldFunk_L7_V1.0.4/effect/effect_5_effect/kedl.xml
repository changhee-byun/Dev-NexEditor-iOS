    <scale y="-1">
      <translate offset="-360 -640 -1545.09668">
        <translate offset="360 640">
          <rect texture="@video1" bounds="-360 -640 360 640" />
        </translate>
        <translate offset="360 640">
          <scale uniform="@anim_1">
            <style alpha="@anim_2">
              <rect texture="@video1" bounds="-360 -640 360 640" />
            </style>
          </scale>
        </translate>
        <translate offset="360 640">
          <rect
            bounds="-360 -640 360 640"
            color="1 0.392 0.196 1.0"
            renderitem="@radialblur"
