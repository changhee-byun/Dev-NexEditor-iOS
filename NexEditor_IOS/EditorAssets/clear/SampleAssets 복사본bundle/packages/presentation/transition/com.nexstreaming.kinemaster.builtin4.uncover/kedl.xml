<effect categorytitle="@string/category_pres_trans" defaultduration="2000" effectoffset="100" icon="uncover.9654880d04aef1592026dc4571bcd8880bde13f5.png" id="com.nexstreaming.kinemaster.builtin4.uncover" maxduration="5000" minduration="500" name="@string/trans_uncover" type="transition" videooverlap="100">
  
