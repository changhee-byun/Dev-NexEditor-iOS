<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/appBackground"
    tools:context="com.nexstreaming.nexdemo.fragment.kmSTTextFragment">

    <ScrollView
        android:id="@+id/containView"
        android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout
            android:layout_width="match_parent" android:layout_height="wrap_content"
            android:orientation="vertical">
            <View
                android:layout_width="match_parent" android:layout_height="21dp"/>
            <LinearLayout
                android:layout_width="match_parent" android:layout_height="80dp">
                <FrameLayout
                    android:layout_width="0dp" android:layout_height="80dp"
                    android:layout_weight="1">
                    <com.nexstreaming.editordemo.widget.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="80dp" android:layout_height="80dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_gravity="center"
                        android:id="@+id/ta_id_1"/>
                </FrameLayout>
                <FrameLayout
                    android:layout_width="0dp" android:layout_height="80dp"
                    android:layout_weight="1">
                    <com.nexstreaming.editordemo.widget.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="80dp" android:layout_height="80dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_gravity="center"
                        android:id="@+id/ta_id_2"/>
                </FrameLayout>
                <FrameLayout
                    android:layout_width="0dp" android:layout_height="80dp"
                    android:layout_weight="1">
                    <com.nexstreaming.editordemo.widget.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="80dp" android:layout_height="80dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_gravity="center"
                        android:id="@+id/ta_id_3"/>
                </FrameLayout>
                <FrameLayout
                    android:layout_width="0dp" android:layout_height="80dp"
                    android:layout_weight="1">
                    <com.nexstreaming.editordemo.widget.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="80dp" android:layout_height="80dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_gravity="center"
                        android:id="@+id/ta_id_4"/>
                </FrameLayout>
            </LinearLayout>
            <View
                android:layout_width="match_parent" android:layout_height="12dp"/>
            <LinearLayout
                android:layout_width="match_parent" android:layout_height="80dp">
                <FrameLayout
                    android:layout_width="0dp" android:layout_height="80dp"
                    android:layout_weight="1">
                    <com.nexstreaming.editordemo.widget.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="80dp" android:layout_height="80dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_gravity="center"
                        android:id="@+id/ta_id_5"/>
                </FrameLayout>
                <FrameLayout
                    android:layout_width="0dp" android:layout_height="80dp"
                    android:layout_weight="1">
                    <com.nexstreaming.editordemo.widget.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="80dp" android:layout_height="80dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_gravity="center"
                        android:id="@+id/ta_id_6"/>
                </FrameLayout>
                <FrameLayout
                    android:layout_width="0dp" android:layout_height="80dp"
                    android:layout_weight="1">
                    <com.nexstreaming.editordemo.widget.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="80dp" android:layout_height="80dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_gravity="center"
                        android:id="@+id/ta_id_7"/>
                </FrameLayout>
                <FrameLayout
                    android:layout_width="0dp" android:layout_height="80dp"
                    android:layout_weight="1">
                    <com.nexstreaming.editordemo.widget.CircleImageView
                        xmlns:app="http://schemas.android.com/apk/res-auto"
                        android:layout_width="80dp" android:layout_height="80dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:layout_gravity="center"
                        android:id="@+id/ta_id_8"/>
                </FrameLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent" android:layout_height="21dp"
                android:gravity="center">
                <ImageView
                    android:layout_width="wrap_content" android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:id="@+id/page_id_1"/>
                <View
                    android:layout_width="3dp" android:layout_height="match_parent"
                    android:layout_gravity="center"/>
                <ImageView
                    android:layout_width="wrap_content" android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:id="@+id/page_id_2"/>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</FrameLayout>
