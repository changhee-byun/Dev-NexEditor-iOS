<effect 
	id="com.nexstreaming.kmsdk.lg.family_2v1.middleillusion" 
	type="title" 
  intime="1500"
  outtime="0"
  intimefirst="1500"
  outtimelast="0"
  repeat="no"
  name="Family Middle Illusion">
  
  <texture id="video1" video="1" />

  <animatedvalue id="ghost_slide1" timingfunction="ease-in-out" scope="whole">
    <keyframe time="0" value="765 360" />
    <keyframe time="0.05" value="765 360" />
    <keyframe time="0.1" value="675 360" />
    <keyframe time="0.23" value="720 360" />
    <keyframe time="0.25" value="753.75 360" />
    <keyframe time="0.28 " value="787.5 360" />
    <keyframe time="0.3" value="506.25 360" />
    <keyframe time="0.85" value="956.25 360" />
    <keyframe time="0.9" value="731.25 360" />
    <keyframe time="1" value="720 360" />
  </animatedvalue>

  <animatedvalue id="ghost_alpha1" timingfunction="ease-in-out" scope="whole">
    <keyframe time="0" value="0.5" />
    <keyframe time="0.05" value="0.4" />
    <keyframe time="0.1" value="0.7" />
    <keyframe time="0.15" value="0.6" />
    <keyframe time="0.25" value="0.7" />
    <keyframe time="0.28 " value="0.4" />
    <keyframe time="0.35" value="0.35" />
    <keyframe time="0.48" value="0.57" />
    <keyframe time="0.5" value="0.65" />
    <keyframe time="0.85" value="0.56" />
    <keyframe time="0.98" value="0.6" />
    <keyframe time="1" value="0.6" />
  </animatedvalue>

  
  <animatedvalue id="clipangle" basis="clipindexshuffle" timingfunction="linear">
    <keyframe value="0" />
    <keyframe value="360" />
  </animatedvalue>
  


  <projection
    type="perspective"
    fov="45"
    aspect="2"
    near="0.01"
    far="60000">
    <scale y="-1">
      <translate offset="-720 -360 -870">
        <translate offset="720 360">
          <rect bounds="-720 -360 720 360" texture="@video1" />
        </translate>


        <translate offset="1080 720">
          <style alpha="@ghost_alpha1">
            <rect bounds="-1440 -720 1440 720" texture="@video1" />
          </style>
        </translate>
	                
      </translate>
    </scale>
  </projection>
</effect>
