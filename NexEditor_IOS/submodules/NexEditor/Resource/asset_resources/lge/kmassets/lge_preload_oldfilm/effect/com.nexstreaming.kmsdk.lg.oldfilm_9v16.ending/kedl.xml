<effect 
	id="com.nexstreaming.kmsdk.lg.oldfilm_9v16.ending" 
	type="title" 
	intime="800" 
	intimefirst="800" 
	outtime="800" 
	outtimelast="800"
	cycletime="800" repeat="yes"
  name="Old Film Ending 9v16">

  <texture id="frame" src="lg_photo_frame.png" />
  <texture id="noise" src="lg_noise.png" animated="2 4 10" />
  <texture id="video1" video="1" />

  <animatedvalue id="frame" timingfunction="linear">
    <keyframe value="1" />
    <keyframe value="10" />
  </animatedvalue>
  
  <animatedvalue id="ending" timingfunction="linear" scope="whole" >
        <part type="in">
        <keyframe value="0" />
        </part>
        
        <part type="mid">
        <keyframe time="0" value="0" />
        <keyframe time="0.7" value="0" />
        <keyframe time="1" value="1" />
        </part>
        
        <part type="out">
        <keyframe value="1" />
        </part>
  </animatedvalue>  
  
  <animatedvalue id="anim_1" timingfunction="ease-in">
    <part type="in">
      <keyframe time="0" value="360 640" />
      <keyframe time="0.2" value="360 640" />
      <keyframe time="0.8" value="-360 640" />
      <keyframe time="1" value="-360 640" />
    </part>
    <part type="mid">
      <keyframe value="-360 640" />
    </part>
    <part type="out">
      <keyframe value="-360 640" />
    </part>
  </animatedvalue>
  
  <projection type="perspective" fov="45" aspect="0.5625" near="0.01" far="60000">
    <scale y="-1">
      <translate offset="-360 -640 -1546.666667">
        <translate offset="@anim_1">
          <translate offset="0 0">
            <!-- Group -->
              <translate offset="0 0">
                <rect bounds="-360 -640 360 640" texture="@video1" />
                <style alpha="0.5" animframe="@frame">
                  <rect bounds="-360 -640 360 640" texture="@noise" />
                </style>
                <rect bounds="-360 -640 360 640" texture="@frame" />  
              </translate>

              <translate offset="720 0">
                <rect bounds="-360 -640 360 640" texture="@video1" />
                <style alpha="0.5" animframe="@frame">
                  <rect bounds="-360 -640 360 640" texture="@noise" />
                </style>
                <rect bounds="-360 -640 360 640" texture="@frame" />  
                <style alpha="@ending">
                  <rect bounds="-360 -640 360 640" color="0 0 0" />
                </style>
             </translate>
            <!-- end Group -->
          </translate>
        </translate>
      </translate>
    </scale>
  </projection>
</effect>