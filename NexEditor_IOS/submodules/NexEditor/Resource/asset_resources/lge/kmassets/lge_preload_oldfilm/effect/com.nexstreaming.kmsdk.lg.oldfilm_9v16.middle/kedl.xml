<effect 
	id="com.nexstreaming.kmsdk.lg.oldfilm_9v16.middle" 
	type="title" 
	repeat="yes"
	intime="1600" 
	intimefirst="1600"
	outtime="800" 
	outtimelast="800"
	cycleTime="800"
	name="Old Film Middle 9v16">
	
 <texture id="frame" src="lg_photo_frame.png" />
  <texture id="noise" src="lg_noise.png" animated="2 4 10" />
  <texture id="video1" video="1" />

  <animatedvalue id="frame" timingfunction="linear">
    <keyframe value="1" />
    <keyframe value="10" />
  </animatedvalue>
  
  <animatedvalue id="anim_1" timingfunction="ease-in">
        <part type="in">
          <keyframe time="0" value="360 640" />
          <keyframe time="0.2" value="360 640" />
          <keyframe time="0.8" value="360 1920" />
          <keyframe time="1" value="360 1920" />
        </part>
        <part type="mid">
         <keyframe value="360 1920" />
        </part>
        <part type="out">
         <keyframe value="360 1920" />
        </part>
  </animatedvalue>
      
  <projection type="perspective" fov="45" aspect="0.5625" near="0.01" far="60000">
    <scale y="-1">
      <translate offset="-360 -640 -1546.666667">
        <translate offset="@anim_1">
          <translate offset="0 0">
            <!-- Group -->
              <translate offset="0 -1280">
                <rect bounds="-360 -640 360 640" texture="@video1" />
                <style alpha="0.5" animframe="@frame">
                  <rect bounds="-360 -640 360 640" texture="@noise" />
                </style>
                <rect bounds="-360 -640 360 640" texture="@frame" />  
            </translate>
              <translate offset="0 0">
                <rect bounds="-360 -640 360 640" texture="@video1" />
                <style alpha="0.5" animframe="@frame">
                  <rect bounds="-360 -640 360 640" texture="@noise" />
                </style>
                <rect bounds="-360 -640 360 640" texture="@frame" />  
              </translate>
              <translate offset="0 1280">
                <rect bounds="-360 -640 360 640" texture="@video1" />
                <style alpha="0.5" animframe="@frame">
                  <rect bounds="-360 -640 360 640" texture="@noise" />
                </style>
                <rect bounds="-360 -640 360 640" texture="@frame" />  
              </translate>
            <!-- end Group -->
          </translate>
        </translate>
      </translate>
    </scale>
  </projection>
</effect>
