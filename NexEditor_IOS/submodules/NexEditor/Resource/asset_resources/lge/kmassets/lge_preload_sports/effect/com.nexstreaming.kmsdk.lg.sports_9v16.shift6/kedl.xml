                  <rotate axis="0 0 1" angle="-60.4">
                    <style alpha="0.4">
                      <rect bounds="-1600 -1100 1600 1100" color="1 0.392 0.196" />
                    </style>
                  </rotate>
                </translate>
                <translate offset="-476.264 -553.286">
                  <rotate axis="0 0 1" angle="-60.4">
                    <rect bounds="-1100 -3 1100 3" color="1 1 1" />
                  </rotate>
                </translate>
              <!-- end Group -->
            </scale>
          </translate>
        </translate>

        <style alpha="@alpha_second" >
          <translate offset="@anim_2">
            <trianglestrip >
              <point location="0 1934" color="#ffffff" />
              <point location="2198 1934" color="#ffffff" />
              <point location="1099 0" color="#ffffff" />
