<effect id="com.nexstreaming.kinemaster.builtin.transition.checker" name="Checker Flip" icon="transition_checker_flip.png" categorytitle="3D Transitions"  type="transition" effectoffset="100" videooverlap="100" >
	<animatedvalue id="spin">
		<keyframe value="0" />
		<keyframe value="180" />
	</animatedvalue>
	<rect bounds="-1 1 1 -1" texture="@video_in" />
	<projection type="perspective" aspect="1.77777" fov="45" top="1" near="1" far="30">
		<cull face="back">
			<translate offset="-15 -8 -21.69">
				<translate offset="0 2.0 0" repeat="9" textureoffset="0 -0.1111111111">
					<translate offset="2.0 0 0" repeat="16" textureoffset="0.0625 0">
						<rotate axis="1 1 0" angle="@spin">
							<trianglestrip texture="@video_out">
								<point location="-1 -1" texcoord="0 1" />
								<point location=" 1 -1" texcoord="0.0625 1" />
								<point location="-1  1" texcoord="0 0.8888888888" />
								<point location=" 1  1" texcoord="0.0625 0.8888888888" />
							</trianglestrip>
						</rotate>
					</translate>
				</translate>
			</translate>
		</cull>
		<!--<cull face="front">
			<translate offset="-15 -8 -21.69">
				<translate offset="0 2.0 0" repeat="9" textureoffset="0 -0.1111111111">
					<translate offset="2.0 0 0" repeat="16" textureoffset="0.0625 0">
						<rotate axis="1 1 0" angle="@spin">
							<trianglestrip texture="@video_in">
								<point location="-1 -1" texcoord="0 1" />
