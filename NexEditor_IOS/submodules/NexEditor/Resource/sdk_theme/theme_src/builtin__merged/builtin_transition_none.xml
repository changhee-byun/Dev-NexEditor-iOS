<effect id="none" 
		name="None" 
		icon="transition_none.png" 
		type="transition" 
		effectoffset="50" 
		videooverlap="0" 
		minduration="0" 
		maxduration="0" 
		defaultduration="0"
		>
		
	<animatedvalue id="pop_in" timingfunction="linear">
		<keyframe value="0" time="0" />
		<keyframe value="0" time="0.5" />
		<keyframe value="1" time="0.5" />
		<keyframe value="1" time="1" />
	</animatedvalue>
	<rect bounds="-1 1 1 -1" texture="@video_out" />
	<style alpha="@pop_in">
		<rect bounds="-1 1 1 -1" texture="@video_in" />
	</style>
	
</effect>
