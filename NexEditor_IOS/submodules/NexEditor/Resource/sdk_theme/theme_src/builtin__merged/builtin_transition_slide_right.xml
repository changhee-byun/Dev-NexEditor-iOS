<effect id="com.nexstreaming.kinemaster.builtin.transition.slideright" name="Slide Right" icon="transition_s_right.png" categorytitle="Presentation"  type="transition" effectoffset="100" videooverlap="100" minduration="500" maxduration="8000" >
	<animatedvalue id="slide_amount">
		<keyframe value="-2 0" />
		<keyframe value="0 0" />
	</animatedvalue>
	<translate offset="@slide_amount">
		<rect bounds="-1 1 1 -1" texture="@video_in" />
		<rect bounds="1 1 3 -1" texture="@video_out" />
	</translate>
</effect>
