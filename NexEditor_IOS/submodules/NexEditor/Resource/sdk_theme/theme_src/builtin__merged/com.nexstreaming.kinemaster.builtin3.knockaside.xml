<effect
  id="com.nexstreaming.kinemaster.builtin3.knockaside"
  type="transition"
  effectoffset="100"
  videooverlap="100"
  minduration="500"
  maxduration="5000"
  defaultduration="2000"
  icon="transition_knock_aside.png"
  categorytitle="Fun Transitions"
  name="Knock Aside">
  
  <animatedvalue id="v1slide" timingfunction="linear">
    <keyframe time="0.45" value="640 360" />
    <keyframe time="0.50" value="-210.989 360" timingfunction="ease-in" />
  </animatedvalue>
  <animatedvalue id="v1scale" timingfunction="linear">
    <keyframe time="0.0" value="1" />
    <keyframe time="0.4" value="0.300" timingfunction="ease-in-out" />
  </animatedvalue>
  <animatedvalue id="v2slide" timingfunction="linear">
    <keyframe time="0.33" value="1486.161 360" />
    <keyframe time="0.45" value="1025 360" timingfunction="ease-in" />
    <keyframe time="0.50" value="1050 360" timingfunction="ease-out" />
    <keyframe time="0.60" value="1050 360" timingfunction="ease-in-out" />
    <keyframe time="0.78" value="640 360" timingfunction="ease-in-out" />
  </animatedvalue>
  <animatedvalue id="v2scale" timingfunction="linear">
    <keyframe time="0.74" value="0.300" />
    <keyframe time="1.0" value="1"  timingfunction="ease-in-out" />
  </animatedvalue>
  <texture id="video1" video="1" />
  <texture id="video2" video="2" />
  <projection
    type="perspective"
    fov="45"
    aspect="1.777777"
    near="0.01"
    far="60000">
    <scale y="-1">
      <translate offset="-640 -360 -870">
        <translate offset="@v1slide">
          <scale uniform="@v1scale">
            <rect bounds="-640 -360 640 360" texture="@video1" />
          </scale>
        </translate>
        <translate offset="@v2slide">
          <scale uniform="@v2scale">
            <rect bounds="-640 -360 640 360" texture="@video2" />
          </scale>
        </translate>
      </translate>
    </scale>
  </projection>
</effect>