<effect
  id="com.nexstreaming.kinemaster.builtin3.terminaltrans"
  type="transition"
  effectoffset="50"
  videooverlap="0"
  minduration="500"
  maxduration="10000"
  defaultduration="2000"
  categorytitle="Text Transitions"
  icon="transition_retro_title.png"
  name="Retro Terminal Title">
  
  
  <userfield id="f_text" type="text">
	<fieldlabel locale="en" value="Title Text" />
  </userfield>
  
  <animatedvalue id="cslide" timingfunction="linear">
    <keyframe time="0.15" value="1748.500 360" />
    <keyframe time="0.25" value="716.998 360" timingfunction="default" />
    <keyframe time="0.75" value="716.998 360" timingfunction="linear" />
    <keyframe time="0.85" value="-480 360" timingfunction="default" />
  </animatedvalue>
  <animatedvalue id="leftscale">
    <keyframe time="0.0" value="7" />
    <keyframe time="0.1" value="1" />
  </animatedvalue>
  <animatedvalue id="rightscale">
    <keyframe time="0.9" value="1" />
    <keyframe time="1.0" value="7" />
  </animatedvalue>
  <animatedvalue id="textslide" timingfunction="linear">
    <keyframe time="0.24" value="1907.385 311.119" />
    <keyframe time="0.31" value="641.451 311.119" timingfunction="default" />
    <keyframe time="0.69" value="641.451 311.119" timingfunction="linear" />
    <keyframe time="0.76" value="-633.861 311.119" timingfunction="default" />
  </animatedvalue>
  <texture id="video2" video="2" />
  <texture id="video1" video="1" />

  <texture
    id="text1"
    textsize="90"
    height="150"
    width="1208"
    fillcolor="#00FF00"
    shadowcolor="#00FF00"
	shadowoffset="0 0"  
	shadowradius="6"  
	shadow="true"
    fill="true"
    textalign="center"
    textvalign="center"
    autosize="false"
	text="%1" 
	srcfield="@f_text"  
	typeface="asset:orbitron-bold.ttf"
    />
  <projection
    type="perspective"
    fov="45"
    aspect="1.777777"
    near="0.01"
    far="60000">
    <scale y="-1">
      <translate offset="-640 -360 -870">
        <translate offset="640 360">
          <rect bounds="-640 -360 640 360" color="0.18 0.18 0.18" />
        </translate>
        <translate offset="@cslide">
          <translate offset="0 0">
            <!-- Group_3 -->
              <translate offset="21.500 0">
                <rect bounds="-1000 -10 1000 10" color="0 1 0" />
              </translate>
              <translate offset="-1108.500 0">
                <scale uniform="@leftscale">
                  <translate offset="0 0">
                    <!-- Group_1 -->
                      <circle center="0 0" color="0 1 0" radius="130.500" />
       				  <style dest="stencil">
	                      <circle center="0 0" color="0 0 0" radius="115.467" />
                      </style>
                    <!-- end Group_1 -->
                  </translate>
                </scale>
              </translate>
              <translate offset="1108.500 0">
                <scale uniform="@rightscale">
                  <translate offset="0 0">
                    <!-- Group_2 -->
                      <circle center="0 0" color="0 1 0" radius="130.500" />
       				  <style dest="stencil">
	                      <circle center="0 0" color="0 0 0" radius="115.467" />
                      </style>
                    <!-- end Group_2 -->
                  </translate>
                </scale>
              </translate>
            <!-- end Group_3 -->
          </translate>
        </translate>
        <translate offset="@textslide">
          <translate offset="0 -25">
            <!-- Group_4 -->
              <translate offset="-1.549 -1.881">
                <rect bounds="-604 -75 604 75" texture="@text1" />
              </translate>
            <!-- end Group_4 -->
          </translate>
        </translate>
        
        <sequence id="sl" start="0" end="720" count="180">
        	<rect bounds="0 @sl 1280 @sl+2" color="0 0 0 0.25" />
        </sequence>
        
        <style rendertest="stencil">
			<visibility start="0" end="0.5">
				<translate offset="640 360">
				  <rect bounds="-640 -360 640 360" texture="@video1" />
				</translate>
			</visibility>
			<visibility start="0.5" end="1">
				<translate offset="640 360">
				  <rect bounds="-640 -360 640 360" texture="@video2" />
				</translate>
			</visibility>
        </style>
      </translate>
    </scale>
  </projection>
</effect>